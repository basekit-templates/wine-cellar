{% extends "layout.twig" %}
{% block content %}

<div class="site-container  page--default">
    <section class="page  page--default">
    
    	<!-- CONTAINER -->
    	<div class="template-container">
    	
    		<!-- HEADER -->
    		{% include "includes/header.twig" %}
    
            <div class="template-title-wrap">
                <div class="template-title">
                    <h2 style="padding-bottom: 0;">
                        <span>{{ page.title }}</span>
                    </h2>
                </div>
            </div>
    	
    		<!-- MAIN SECTION -->
    		<div role="main" class="ecom-product">
    	
    			{{zone('main')|raw}}
    	
    		</div>
    		<!-- END MAIN SECTION -->
    	
    	</div>
    	<!-- END CONTAINER -->
    
    </section>

<!-- FOOTER -->
{% include "includes/footer.twig" %}

</div>

{% endblock %}
