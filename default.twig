{% extends "layout.twig" %}

{% block content %}

<!-- Site Container (Required) -->
<div class="site-container  page--default">
    <section class="page">
    
    
        {% include "includes/header.twig" %}
    
        <div class="template-title-wrap">
            <div class="template-title">
                {{widget('pagetitle', 'title', {})|raw}}
            </div>
        </div>
    
    
        <!-- Zone Content -->
        <div class="main-content">
            <div class="container">
    
                {{zone('main')|raw}}
    
            </div>
        </div>
    
    </section>

{% include "includes/footer.twig" %}

</div>

{% endblock %}

